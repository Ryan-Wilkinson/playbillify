<%= form_for [@organization, @event, @ad] do |f| %>
  <p>
    <%= f.label :size %><br>
    <%= f.text_field :size %>
  </p>
  <p>
    <%= f.label :price %><br>
    <%= f.number_field :price %>
  </p>
  <p>
    <%= f.label :photo_url %><br>
    <%= f.text_field :photo_url %>
  </p>
  <p>
    <%= f.label :dimensions %><br>
    <%= f.text_field :dimensions %>
  </p>

 <p>
    <%= f.submit %>
  </p>
<% end %>




