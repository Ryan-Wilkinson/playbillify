<div class='page-content'>
  <h4 class="center">Browse available ads</h4>
  <br>
  <div class='row'>
    <div class="col s10">
      <%@events.each_with_index do |event, index| %>
        <%if event.ads.select{|ad| ad.advertiser_id == nil}.size > 0 %>
          <%= render 'accordion', event: event, i: index%>
        <%end %>
      <%end %>
    </div>
    <div class='col s2'>
      <div class="card-panel white lighten-5 z-depth-1">
        <div class="row">
          <div>Search</div>
          <input></input>
          <div>Filter</div>
          <input></input>
        </div>
      </div>
    </div>
  </div>
</div>

